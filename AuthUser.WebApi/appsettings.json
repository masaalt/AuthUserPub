{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "AppS3Bucket": "",
  "IsCloudDeployment": "false",
  "Region": "ap-south-1",
  "DBProvider": "mssql",
  "ConnectionStrings": {
    "MSSQLConnection": "Data Source=.;Initial Catalog=----DbName---;Integrated Security=True;"
  },
  "JWTSettings": {
    "Key": "---Key---",
    "Issuer": "AuthUser",
    "Audience": "AuthUserUser",
    "DurationInMinutes": 30,
    "TokenLifetimeInMin": 30
  },
  "AllowedHosts": "*"
}